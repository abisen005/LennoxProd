                            </apex:outputpanel>
                            <apex:outputPanel id="fDRV" rendered="{! Site_Visits__c.F_difference_reversing_valve__c!=null}">
                            <div class="slds-m-top_medium">
                                <div class="slds-text-title">F difference Reversing Value </div>
                                <div class="mincontent-h">{! Site_Visits__c.F_difference_reversing_valve__c}</div>
                            </div>
