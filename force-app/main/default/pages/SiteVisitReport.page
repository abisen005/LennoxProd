                                <div class="mincontent-h">{! Site_Visits__c.Discharge_psig_Compressor__c}</div>
                            </div>
                            </apex:outputpanel>
                            <apex:outputPanel id="sPC" rendered="{! Site_Visits__c.Suction_psig_Compressor__c!=null}">
                            <div class="page-break" style="page-break-after:always;"></div>
                            <div class="slds-m-top_medium">
                                <div class="slds-text-title">Suction psig @ compresor </div>
                                <div class="mincontent-h">{! Site_Visits__c.Suction_psig_Compressor__c}</div>
                            </div>
                            </apex:outputpanel>
                            <apex:outputPanel id="cACLO" rendered="{! Site_Visits__c.Compressor_Amps_C_L1__c!=null}">
                            <div class="slds-m-top_medium">
                                <div class="slds-text-title"> Compressor Amps "C"(L1) </div>
                                <div class="mincontent-h">{! Site_Visits__c.Compressor_Amps_C_L1__c}</div>
                            </div>
                            </apex:outputpanel>
                            <apex:outputPanel id="cACLTW" rendered="{! Site_Visits__c.Compressor_Amps_R_L2__c!=null}">
                            <div class="slds-m-top_medium">
                                <div class="slds-text-title"> Compressor Amps "R"(L2) </div>
